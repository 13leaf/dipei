{% spaceless %}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta charset="utf-8" />
        <link rel="icon" href="/favicon.ico" type="image/x-icon">
        <link rel="bookmark" href="/favicon.ico" type="image/x-icon">
        <title>{% block title %}{{_e('鲜旅客 –带你寻找鲜の旅')}}{% endblock %}</title>
        {# render base css , this is base file for all site #}
        {{ sta( 'base.css') }}

        {# include follow base css  #}
        {% block head %}
        {{ sta( page_css_list )}}
        {% endblock %}
        <script type="text/javascript">
            var LP_CONFIGS = {
                uid: '{{UID}}'
            }
        </script>
        {# render base js, this is base js file for the whole site #}
        {{ sta( 'sea/sea-debug.js,config.js,sea/plugin-shim.js,lp.core.js,lp.base.js' ) }}
        <!--[if lt IE 9]>
        <script type="text/javascript">
            (function(){var tmps = 'article,aside,details,figcaption,figure,footer,header,hgroup,main,nav,section,summary'.split(',');for(var tmp,i=0;tmp=tmps[i];i++){document.createElement(tmp);}})();
        </script>
        <![endif]-->

    </head>
    <body {% block bodyclass %}{% endblock %}>
        <header>
            {% include "base/header.twig" %}
            {% block header %}{% endblock %}
        </header>
        <div id="content">{% block content %}{% endblock %}</div>
        <footer>
            {% block footer %}{% endblock %}
            {% include "base/footer.twig" %}
        </footer>
        {% if UID %}
        {% include "widget/like-to-bar.twig" %}
        {% endif %}
        {# render page js and pass server var to js #}
        {{renderPageJs()}}
        <script type="text/javascript">
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
